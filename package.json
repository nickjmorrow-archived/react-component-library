{
  "name": "@nickjmorrow/react-component-library",
  "version": "0.0.5",
  "description": "Personal component library",
  "author": "nickjmorrow",
  "license": "MIT",
  "repository": "nickjmorrow/react-component-library",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm run build",
    "clear": "rm -rf node_modules package-lock.json example/node_modules example/package-lock.json && npm install && npm install ./examples",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "deepmerge": "^3.2.0",
    "emitter": "0.0.5",
    "radium": "^0.25.1",
    "rc-slider": "^8.6.5",
    "react-animations": "^1.0.0",
    "react-google-login": "^5.0.0",
    "react-media": "^1.9.2",
    "react-modal": "^3.8.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.5.1",
    "react-transition-group": "^2.6.0",
    "stream": "0.0.2",
    "ts-node": "^8.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/radium": "^0.24.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-modal": "^3.8.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^2.0.16",
    "@types/styled-components": "^4.1.4",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "module-alias": "^2.2.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.2",
    "rollup-plugin-url": "^1.4.0",
    "ts-essentials": "^2.0.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.3.1"
  },
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~": "dist"
  }
}
